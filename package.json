{
  "name": "fb-utm-generator",
  "version": "1.0.0",
  "description": "A web app for generating UTM parameters for Facebook Ads",
  "main": "app.js",
  "scripts": {
    "server": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm build && git add . && git commit -m \"Building for production\" && git push",
    "build": "cd client && npm run build",
    "start": "concurrently --kill-others \"nodemon app.js\" \"npm run client\"",
    "client": "cd client && npm run start"
  },
  "author": "Grant Stoltz",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "csvtojson": "^2.0.7",
    "express": "^4.16.3",
    "fast-csv": "^2.4.1",
    "json-2-csv": "^2.2.1",
    "multer": "^1.3.1",
    "nodemon": "^1.18.3",
    "prompt": "^1.0.0"
  }
}
